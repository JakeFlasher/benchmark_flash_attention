  -a num_seqs=$S -a num_heads=$Hqo -a num_kv_heads=$Hkv -a head_size=$D \
  -a window_left=4096 -a window_right=4096 \
  --disable-blocking-kernel --md "results_standard/deepseek-r1-window.md"
